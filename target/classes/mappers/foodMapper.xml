<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.sinc.mybatis.mapper.food">
  
   	<select id="foodReadRow" parameterType="String" resultType="food" flushCache="true" useCache="true">
		SELECT IMG, FOOD_NAME FROM FOODIMG
		WHERE FOOD_NAME= #{foodName}
	
	</select>
  </mapper>